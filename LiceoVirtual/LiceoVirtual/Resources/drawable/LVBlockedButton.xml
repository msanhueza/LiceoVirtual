<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

	<item>
	<shape  android:shape="rectangle" >
			<corners
			android:radius="18dp"
			/>
			<gradient
android:angle="270"
android:centerX="5%"
android:centerColor="#2868A8"
android:startColor="#2868A8"
android:endColor="#1982B0"
android:type="linear"
/>
			<stroke
			android:width="2dp"
			android:color="#2A596E"
			/>

		</shape> 
	</item>
	   <item android:top="10dp" android:right="10dp">
     <bitmap

          android:src="@drawable/candado"
          android:gravity="top|right"

          />
   </item>

	<item>
	<shape  android:shape="rectangle" >
			<corners
			android:radius="18dp"
			/>
			<solid
			android:color="#000a63af"
			/>
			<stroke
			android:width="2dp"
			android:color="#2A596E"
			/>
		</shape> 
	
	</item>


</layer-list>